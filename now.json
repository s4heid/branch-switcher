{
  "name": "branch-switcher",
  "version": 2,
  "alias": "branch-switcher.now.sh",
  "public": true,
  "env": {
    "APP_ID": "@app-id",
    "WEBHOOK_SECRET": "@webhook-secret",
    "PRIVATE_KEY": "@private-key-base64-encoded",
    "IS_NOW": "true"
  },
  "builds": [
    {
      "src": "./src/*.ts",
      "use": "@now/node@canary"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/src"
    }
  ]
}
