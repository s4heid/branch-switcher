{
  "name": "branch-switcher",
  "version": "1.0.0",
  "contributors": [
    "Danny Berger",
    "Sebastian Heid"
  ],
  "description": "A branch switcher app.",
  "license": "Apache-2.0",
  "repository": "https://github.com/s4heid/branch-switcher.git",
  "homepage": "https://github.com/s4heid/branch-switcher",
  "bugs": "https://github.com/s4heid/branch-switcher/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "nodemon --exec \"npm start\"",
    "start": "probot run ./lib/index.js",
    "lint": "standard **/*.ts --fix",
    "test": "jest && standard **/*.ts",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "probot": "^12.2.8",
    "probot-serverless-now": "^2.1.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/nock": "^11.1.0",
    "@types/node": "^16.4.0",
    "@typescript-eslint/parser": "^4.32.0",
    "@vercel/ncc": "^0.24",
    "acorn": "^8.6.0",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^26.6.3",
    "nock": "^13.1.0",
    "nodemon": "^2.0.13",
    "smee-client": "^1.2.2",
    "standard": "^16.0.4",
    "ts-jest": "^26.4.4",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": ">= 10.13.0"
  },
  "standard": {
    "parser": "@typescript-eslint/parser",
    "env": [
      "jest"
    ],
    "plugins": [
      "typescript"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
