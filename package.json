{
  "name": "branch-switcher",
  "version": "1.0.0",
  "contributors": [
    "Danny Berger",
    "Sebastian Heid"
  ],
  "description": "A branch switcher app.",
  "license": "Apache-2.0",
  "repository": "https://github.com/s4heid/branch-switcher.git",
  "homepage": "https://github.com/s4heid/branch-switcher",
  "bugs": "https://github.com/s4heid/branch-switcher/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "nodemon --exec \"npm start\"",
    "start": "probot run ./lib/index.js",
    "lint": "standard **/*.ts --fix",
    "test": "jest && standard **/*.ts",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "@types/nock": "^11.1.0",
    "nock": "^11.7.2",
    "probot": "^9.9.1",
    "probot-serverless-now": "^2.1.2"
  },
  "devDependencies": {
    "@types/jest": "^25.1.1",
    "@types/node": "^13.1.2",
    "@typescript-eslint/parser": "^2.18.0",
    "acorn": "^7.1.0",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2",
    "smee-client": "^1.0.2",
    "standard": "^14.3.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.4"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "parser": "@typescript-eslint/parser",
    "env": [
      "jest"
    ],
    "plugins": [
      "typescript"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
