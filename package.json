{
  "name": "branch-switcher",
  "version": "1.0.0",
  "contributors": [
    "Danny Berger",
    "Sebastian Heid"
  ],
  "description": "A branch switcher app.",
  "license": "Apache 2.0",
  "repository": "https://github.com/s4heid/branch-switcher.git",
  "homepage": "https://github.com/s4heid/branch-switcher",
  "bugs": "https://github.com/s4heid/branch-switcher/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "nodemon --exec \"npm start\"",
    "start": "probot run ./lib/index.js",
    "lint": "standard **/*.ts --fix",
    "test": "jest && standard **/*.ts",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "@chadfawcett/probot-serverless-now": "^1.0.0",
    "@types/nock": "^10.0.3",
    "nock": "^10.0.0",
    "probot": "^7.2.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.3",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.1",
    "smee-client": "^1.0.2",
    "standard": "^12.0.1",
    "ts-jest": "^24.0.0",
    "typescript": "^3.6.2",
    "typescript-eslint-parser": "^22.0.0"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "parser": "typescript-eslint-parser",
    "env": [
      "jest"
    ],
    "plugins": [
      "typescript"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
